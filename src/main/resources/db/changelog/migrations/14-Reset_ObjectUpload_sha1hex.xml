<?xml version="1.1" encoding="UTF-8" standalone="no" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://local.xsd/dbchangelog-4.4.xsd"
                   objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

  <!-- sha1hex before 1.4 were incorrectly calculated, this can be sued to remove the incorrect data -->
  <changeSet id="14-Reset_ObjectUpload_sha1hex" context="data-migration" author="cgendreau">
    <update tableName="object_upload">
      <column name="sha1_hex" value="0" />
    </update>
  </changeSet>
</databaseChangeLog>
