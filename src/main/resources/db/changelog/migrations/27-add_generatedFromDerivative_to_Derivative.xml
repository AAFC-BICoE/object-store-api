<?xml version="1.1" encoding="UTF-8" standalone="no" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd"
                   objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <changeSet id="27-add_generatedFromDerivative_to_Derivative" context="schema-change" author="KeyukJ">
        <!-- Add generated_from_derivative column -->
        <addColumn tableName="derivative">
            <column name="generated_from_derivative" type="INTEGER"/>
        </addColumn>
        <addForeignKeyConstraint
                baseColumnNames="generated_from_derivative"
                baseTableName="derivative"
                constraintName="fk_derivative_generated_from_derivative"
                referencedColumnNames="id"
                referencedTableName="derivative"/>
    </changeSet>

</databaseChangeLog>
